@model System.Web.Mvc.HandleErrorInfo
<h1 class="text-danger">Error.@Model.Exception.Message</h1>

<h2 class="text-danger">An error occurred while processing your request.</h2>
<h3 class="text-danger">@Model.Exception.InnerException.Message</h3>
<h3 class="text-danger">@Model.Exception.InnerException.StackTrace</h3>
@Html.ActionLink("Back", @Model.ActionName, @Model.ControllerName)
