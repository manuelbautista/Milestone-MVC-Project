                    milestonecomp = $(this).find(".clsmilestonecomp").val();
                    milestonestatus = $(this).find("input[name='radiostatus']:checked").val();
                });

                $.ajax({
                    type: "POST",
                    url: "@Url.Content("~/Project/AddMilestone")",
                    dataType: "json",
